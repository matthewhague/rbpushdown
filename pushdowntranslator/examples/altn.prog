
counter v reversals 0
counter j reversals 0
counter k reversals 1

freevar n

constraint (error > 1)

start main

procedure main()
begin
    while [v < n] do
    begin
        v++;
        k++;
    end;

    while [j < n] do
    begin
        if [k == 0] then
            echo error;
        j++;
        k--;
    end;
end;
